{
    "name": "ttbooking/castable-money",
    "description": "MoneyPHP casts for Laravel",
    "type": "library",
    "keywords": ["money", "castable", "cast"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ttbooking/castable-money/issues",
        "source": "https://github.com/ttbooking/castable-money"
    },
    "authors": [
        {
            "name": "Sergey Danilchenko",
            "email": "s.danilchenko@ttbooking.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/container": "^8.40",
        "illuminate/database": "^8.40",
        "moneyphp/money": "^4.0",
        "ttbooking/circle-of-hell": "^1.0",
        "ttbooking/money-serializer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TTBooking\\CastableMoney\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TTBooking\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
